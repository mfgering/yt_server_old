  "version": "0.2.0",
  "configurations": [
  
    {
      "name": "Python: Flask",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "run.py",
        "FLASK_ENV": "development",
        "FLASK_DEBUG": "0",
        "PYTHONPATH": "${workspaceFolder}/youtube_dl_downloader;${workspaceFolder}/yt_dlp_downloader"
      },
      "args": [
        "run",
        "--no-debugger",
        "--no-reload"
      ],
      "jinja": true,
      "justMyCode": false
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Test",
      "type": "python",
      "request": "launch",
      "program": "test_extractor.py",
      "console": "integratedTerminal",
    }
  ]
}